{
  "name": "next-sst",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "sst bind next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test:e2e:install": "playwright install --with-deps",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:unit": "jest --coverage --passWithNoTests",
    "test:unit:watch": "jest --coverage --passWithNoTests --watch",
    "check:types": "tsc -p ./tsconfig.json --pretty --noEmit",
    "check:deps": "depcruise src e2e",
    "pipeline": "yarn run prettier && yarn run lint && yarn run check:types && yarn run check:deps && yarn run test:unit && yarn run test:e2e",
    "deploy:dev": "sst deploy --stage development",
    "deploy:prod": "sst deploy --stage production",
    "prepare": "husky install"
  },
